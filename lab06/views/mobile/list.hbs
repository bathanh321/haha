<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Image</th>
    </tr>
    {{#each mobiles as |mobile|}}
    <tr>
        <td>{{mobile.number}}</td>
        <td>{{mobile.mobile}}</td>
        <td><img src="{{mobile.image}}" alt="Mobile Image" width="100" height="100"></td>
    </tr>
    {{/each}}
</table>
